{
	"presets": [
		"@emotion/babel-preset-css-prop",
		["next/babel", {
			"preset-react": {
				"runtime": "automatic",
				"importSource": "@emotion/react"
			}
		}]
	],
	"plugins": [
		"@emotion/babel-plugin",
		[
			"module-resolver", {
				"alias": {
					"@": "./src"
				}
			}
		]
	],
	"env": {
		"test": {
			"plugins": ["babel-plugin-dynamic-import-node"]
		}
	}
}
